                    </ui:VisualElement>
                    <ui:VisualElement style="width: 50%;">
                        <ui:Label text="WASD" display-tooltip-when-elided="true" style="margin-top: 83px; -unity-text-align: upper-left; margin-left: 30px;" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement style="flex-direction: row; align-items: flex-start; justify-content: center; width: 100%;">
                    <ui:VisualElement style="width: 50%;">
                        <ui:Label text="Aim:" display-tooltip-when-elided="true" style="-unity-text-align: upper-right;" />
                    </ui:VisualElement>
                    <ui:VisualElement style="align-items: stretch; width: 50%;">
                        <ui:Label text="Mouse" display-tooltip-when-elided="true" style="-unity-text-align: upper-left; margin-left: 30px;" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement style="flex-direction: row; align-items: flex-start; justify-content: center; width: 100%;">
                    <ui:VisualElement style="width: 50%;">
                        <ui:Label text="Heal:" display-tooltip-when-elided="true" style="-unity-text-align: upper-right;" />
                    </ui:VisualElement>
                    <ui:VisualElement style="align-items: stretch; width: 50%;">
                        <ui:Label text="Left MB" display-tooltip-when-elided="true" style="-unity-text-align: upper-left; margin-left: 15px;" />
                    </ui:VisualElement>
                </ui:VisualElement>
