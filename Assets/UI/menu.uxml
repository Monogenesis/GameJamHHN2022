            <ui:Button text="OK" display-tooltip-when-elided="true" name="showrulesback-button" style="position: absolute; left: 85%; top: auto; height: 10%; width: 10%; border-top-left-radius: 0; border-bottom-left-radius: 0; border-top-right-radius: 0; border-bottom-right-radius: 0; font-size: 59px; bottom: 5%; display: flex;" />
