        <ui:VisualElement name="rules-page" style="width: 90%; height: 90%; align-items: center; display: none; background-color: rgb(84, 74, 74);">
            <ui:VisualElement style="flex-direction: column; width: 100%; padding-bottom: 16px;">
                <ui:Label text="How To Play" display-tooltip-when-elided="true" style="font-size: 105px; -unity-text-align: upper-center; border-bottom-width: 5px; border-bottom-color: rgb(255, 255, 255);" />
                <ui:VisualElement style="white-space: normal; flex-wrap: nowrap; overflow: visible;">
                    <ui:Label text="The tribes are at war! Don&apos;t let just one tribe conquer &#10;this world!" display-tooltip-when-elided="true" style="flex-wrap: wrap; overflow: visible; -unity-text-align: upper-center; font-size: 67px; -unity-font-style: normal;" />
                    <ui:Label text="- Try to keep the tribes population at balance so no -&#10;tribe becomes the last one left" display-tooltip-when-elided="true" style="flex-wrap: wrap; overflow: visible; -unity-text-align: upper-center; font-size: 60px;" />
                    <ui:Label text="- If only &lt;b&gt;one&lt;/b&gt; tribes survives you failed your mission -" display-tooltip-when-elided="true" style="flex-wrap: wrap; overflow: visible; -unity-text-align: upper-center; font-size: 60px;" />
                    <ui:Label text="Controls:" display-tooltip-when-elided="true" style="flex-wrap: wrap; overflow: visible; -unity-text-align: upper-center; font-size: 60px; margin-top: 59px; border-bottom-width: 5px; border-bottom-color: rgb(255, 255, 255);" />
                </ui:VisualElement>
                <ui:VisualElement style="flex-direction: row; width: 100%;">
                    <ui:VisualElement style="width: 50%;">
