        <ui:VisualElement name="settings-page" style="width: 90%; height: 90%; align-items: center; display: none; background-color: rgb(84, 74, 74);">
            <ui:VisualElement style="flex-direction: column; width: 100%; padding-bottom: 16px;">
                <ui:Label text="Settings" display-tooltip-when-elided="true" style="font-size: 105px; -unity-text-align: upper-center; border-bottom-width: 5px; border-bottom-color: rgb(255, 255, 255);" />
                <ui:VisualElement />
            </ui:VisualElement>
            <ui:Button text="OK" display-tooltip-when-elided="true" name="settingsback-button" style="position: absolute; left: 85%; top: auto; height: 10%; width: 10%; border-top-left-radius: 0; border-bottom-left-radius: 0; border-top-right-radius: 0; border-bottom-right-radius: 0; font-size: 59px; bottom: 5%; display: flex;" />
        </ui:VisualElement>
        <ui:VisualElement name="welcome-page" style="width: 90%; height: 90%; align-items: center; display: flex; background-color: rgb(84, 74, 74); justify-content: center;">
            <ui:VisualElement style="flex-direction: column; width: 100%; padding-bottom: 16px; justify-content: flex-start; align-items: center;">
                <ui:VisualElement />
                <ui:Label text="Balance of War" display-tooltip-when-elided="true" name="welcomescreen-label" class="welcome-spawn" style="font-size: 172px; -unity-text-align: upper-center; border-bottom-color: rgb(255, 255, 255);" />
            </ui:VisualElement>
            <ui:Label text="&lt; press any button &gt;" display-tooltip-when-elided="true" name="welcomescreen-clickany-label" class="fade-in" style="color: rgba(255, 255, 255, 0.76); padding-top: 130px; transition-duration: 3000ms; opacity: 0; transition-delay: 0ms; transition-property: opacity;" />
        </ui:VisualElement>
    </ui:VisualElement>
